{{#each preview}}
	<div class="mod-svgsprite">
		<h3 class="mod-svgsprite__title">Sprite Name: {{@key}}</h3>
		<ul class="mod-svgsprite__list">
			{{#each this}}
			<li class="mod-svgsprite__item">
				<svg class="mod-svgsprite__icon mod-svgsprite__icon--{{this}}" focusable="false">
					<use xlink:href="#{{this}}" />
				</svg>
				<span>
					{{this}}
				</span>
			</li>
			{{/each}}
		</ul>
	</div>
{{/each}}
